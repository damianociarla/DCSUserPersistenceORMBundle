{
  "name": "dcs/user-persistence-orm-bundle",
  "type": "symfony-bundle",
  "description": "The DCSUserPersistenceORMBundle adds functionality to DCSUserCoreBundle and provides the implementation of the persistence on database using Doctrine ORM",
  "require": {
    "php": ">=5.3.9",
    "doctrine/orm": "^2.5",
    "dcs/user-core-bundle": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "DCS\\User\\Persistence\\ORMBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DCS\\User\\Persistence\\ORMBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}