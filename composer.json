{
  "name": "dcs/user-persistence-orm-bundle",
  "type": "symfony-bundle",
  "description": "The DCSUserPersistenceORMBundle adds functionality to DCSUserCoreBundle and provides the implementation of the persistence on database using Doctrine ORM",
  "keywords": ["User management","ORM","Doctrine"],
  "license": "MIT",
  "authors": [
    {
      "name": "Damiano Ciarla",
      "email": "damiano.ciarla@gmail.org"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "symfony/security": "~2.8|~3.0",
    "dcs/user-core-bundle": "~1.0@dev",
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpcov": "3.*"
  },
  "autoload": {
    "psr-4": {
      "DCS\\User\\Persistence\\ORMBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DCS\\User\\Persistence\\ORMBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}