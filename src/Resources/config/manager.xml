<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="dcs_user.persistence.orm.manager.delete.default.class">DCS\User\Persistence\ORMBundle\Manager\Delete</parameter>
        <parameter key="dcs_user.persistence.orm.manager.save.default.class">DCS\User\Persistence\ORMBundle\Manager\Save</parameter>
    </parameters>

    <services>
        <service id="dcs_user.persistence.orm.manager.delete.default" class="%dcs_user.persistence.orm.manager.delete.default.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
        <service id="dcs_user.persistence.orm.manager.save.default" class="%dcs_user.persistence.orm.manager.save.default.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
    </services>
</container>